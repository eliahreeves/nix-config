    device = "/dev/disk/by-uuid/fc31e9a1-a132-442b-aac4-3677f8046587";
    fsType = "ext4";
  };

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/6569-5FD3";
    fsType = "vfat";
    options = ["fmask=0077" "dmask=0077"];
  };

  swapDevices = [
    {device = "/dev/disk/by-uuid/a02f0693-c58c-4ee7-bf4f-5f41f17c2cb6";}
